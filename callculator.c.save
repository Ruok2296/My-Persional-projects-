#include <stdio.h>

int main() {
int n1,n2;
scanf("%d%d",&n1,&n2);
//printf("%d%d",n1,n2);
char s;
scanf("%c",&s);

if(s == '+'){
    printf("%d",n1+n2);
}else if ( s == '-'){
    printf("%d",n1-n2);
}else if(s == '*'){
    printf("%d",n1*n2);
}else if (s == '/'){
    printf("%d",n1/n2);
}
    return 0;
}
